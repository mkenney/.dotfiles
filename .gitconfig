[user]
	email = mkenney@webbedlam.com
	name = Michael Kenney
[diff]
    external = .vimdiff_wrapper
[pager]
    diff =
[color]
	diff = auto
	status = auto
	branch = auto
	interactive = auto
	ui = auto
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
[color "branch"]
	current = green bold
	local = green
	remote = red bold
[color "status"]
	added = green bold
	changed = yellow bold
	untracked = red
[core]
	pager = less
	whitespace = fix,-indent-with-non-tab,trailing-space,cr-at-eol
[apply]
	whitespace = fix
