# Work
alias mount_mbkdev="sshfs mkenney@mbkdev:/home/mkenney ~mkenney/mounts/mbkdev -oauto_cache,reconnect,defer_permissions,negative_vncache,follow_symlinks,transform_symlinks,volname=mbkdev"
alias umount_mbkdev="umount ~mkenney/mounts/mbkdev"

alias mount_mbkdev2="mkdir ~mkenney/mounts/mbkdev2 && sshfs mkenney@mbkdev2:/home/mkenney ~mkenney/mounts/mbkdev2 -oauto_cache,reconnect,defer_permissions,negative_vncache,follow_symlinks,transform_symlinks,volname=mbkdev2"
alias umount_mbkdev2="umount ~mkenney/mounts/mbkdev2 && rm -rf ~mkenney/mounts/mbkdev2"

# Home
alias mount_devserv="sshfs -p 65022 mkenney@dev.9c0.com:/home/mkenney ~mkenney/mounts/devserv -oauto_cache,reconnect,defer_permissions,negative_vncache,follow_symlinks,transform_symlinks,volname=mbkdev"
alias umount_devserv="umount ~mkenney/mounts/devserv"
